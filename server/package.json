{
    "name": "jackal",
    "version": "1.0.0",
    "description": "Jackal",
    "main": "dist/index.js",
    "scripts": {
        "test": "jest --detectOpenHandles --forceExit --silent=false --runInBand --no-cache",
        "run:dev": "cross-env NODE_ENV=development nodemon dist/index.js",
        "dev": "concurrently \"npm run watch\" \"npm run run:dev\"",
        "start": "node dist/index.js",
        "build": "tsc && tsc-alias",
        "watch": "concurrently --kill-others \"tsc --watch\" \"tsc-alias --watch\""
    },
    "keywords": [
        "suricata",
        "network",
        "security",
        "ids",
        "ips",
        "waf",
        "firewall",
        "network-security",
        "cybersecurity",
        "infosec",
        "infosecurity",
        "cyber",
        "networking"
    ],
    "author": "Mohamad Khawam",
    "license": "ISC",
    "dependencies": {
        "@types/color": "^4.2.0",
        "colors": "^1.4.0",
        "express": "^4.21.1",
        "express-file-routing": "^3.0.3",
        "express-ws": "^5.0.2",
        "jest": "^29.7.0",
        "joi": "^17.13.3",
        "just-types": "^2.0.0-alpha.3",
        "lib": "file:../lib",
        "morgan": "^1.10.0",
        "systeminformation": "^5.23.5",
        "tail": "^2.2.6",
        "yaml": "^2.6.1"
    },
    "devDependencies": {
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/lodash.debounce": "^4.0.9",
        "@types/morgan": "^1.9.9",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@types/tail": "^2.2.3",
        "@webpack-cli/generators": "^3.0.7",
        "autoprefixer": "^10.4.20",
        "concurrently": "^9.1.0",
        "cross-env": "^7.0.3",
        "nodemon": "^3.1.7",
        "prettier": "^3.3.3",
        "style-loader": "^4.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.1",
        "tsc-alias": "^1.8.10",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.7.2",
        "workbox-webpack-plugin": "^7.3.0"
    }
}
